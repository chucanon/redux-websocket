{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "max-line-length": {
      "options": [120]
    },
    "ter-arrow-parens": [true, "as-needed"],
    "variable-name": [
      true,
      "allow-pascal-case",
      "allow-leading-underscore"
    ],
    "import-name": [false]
  },
  "rulesDirectory": []
}

